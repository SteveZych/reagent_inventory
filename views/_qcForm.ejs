<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Reagent_ID">Reagent ID</label>
    <div class="col-sm-8">
      <input class="form-control" id="Reagent_ID" name="Reagent_ID" readonly value="<%= model.Reagent_ID %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Reagent_Name">Reagent Name</label>
    <div class="col-sm-8">
      <input class="form-control" id="Reagent_Name" name="Reagent_Name" readonly value="<%= model.Reagent_Name %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Lot_Number">Lot Number</label>
    <div class="col-sm-7">
      <input class="form-control" id="Lot_Number" name="Lot_Number" readonly value="<%= model.Lot_Number %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Receive_Date">Receive Date</label>
    <div class="col-sm-7">
      <input class="form-control" type="date" id="Receive_Date" name="Receive_Date" readonly value="<%= model.Receive_Date %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="QC_Date">QC Date</label>
    <div class="col-sm-7">
      <input class="form-control" type="date" id="QC_Date" name="QC_Date" value="<%= model.QC_Date %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Performed_By">Performed By</label>
    <div class="col-sm-7">
      <input class="form-control" id="Performed_By" name="Performed_By" value="<%= model.Performed_By %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comments</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" id="Comments" name="Comments" maxlength="32000" rows="7"><%= model.Comments %></textarea>
    </div>
  </div>