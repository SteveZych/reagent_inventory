<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Department_Name">Department Name</label>
    <div class="col-sm-8">
      <input class="form-control" id="Department_Name" name="Department_Name" readonly value="<%= model.Department_Name %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Department_Bench">Department Bench</label>
