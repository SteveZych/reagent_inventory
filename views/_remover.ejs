      <input class="form-control" id="Department_Bench" name="Department_Bench" readonly value="<%= model.Department_Bench %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Instrument">Instrument</label>
