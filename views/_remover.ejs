    <label class="col-form-label col-sm-2" for="Received_By">Received By</label>
    <div class="col-sm-7">
      <input class="form-control" id="Received_By" name="Received_By" readonly value="<%= model.Received_By %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comments</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" id="Comments" name="Comments" maxlength="32000" readonly rows="7"><%= model.Comments %></textarea>
    </div>
  </div>