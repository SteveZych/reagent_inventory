<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Department_Name">Department Name</label>
    <div class="col-sm-8">
      <input class="form-control" name="Department_Name" value="<%= model.Department_Name %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Department_Area">Department Area</label>
    <div class="col-sm-8">
      <input class="form-control" name="Department_Area" value="<%= model.Department_Area %>" />
    </div>
  </div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Reagent_Name">Reagent Name</label>
    <div class="col-sm-8">
      <input class="form-control" name="Reagent_Name" value="<%= model.Reagent_Name %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Receive_Date">Receive Date</label>
    <div class="col-sm-7">
      <input class="form-control" type="date" name="Receive_Date" value="<%= model.Receive_Date %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Lot_Number">Lot Number</label>
    <div class="col-sm-7">
      <input class="form-control" name="Lot_Number" value="<%= model.Lot_Number %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Expiration_Date">Expiration Date</label>
    <div class="col-sm-7">
      <input class="form-control" type="date" name="Expiration_Date" value="<%= model.Expiration_Date %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Quantity">Quantity</label>
    <div class="col-sm-7">
      <input class="form-control" name="Quantity" value="<%= model.Quantity %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comments</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" name="Comments" maxlength="32000" rows="7"><%= model.Comments %></textarea>
    </div>
  </div>