<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Reagent_Name">Reagent Name</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="Reagent_Name" value="<%= model.Reagent_Name %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Receive_Date">Receive_Date</label>
    <div class="col-sm-7">
      <input class="form-control" name="Receive_Date" value="<%= model.Receive_Date %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Lot_Number">Lot_Number</label>
    <div class="col-sm-7">
      <input class="form-control" name="Lot_Number" value="<%= model.Lot_Number %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Expiration_Date">Expiration_Date</label>
    <div class="col-sm-7">
      <input class="form-control" name="Expiration_Date" value="<%= model.Expiration_Date %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Quantity">Quantity</label>
    <div class="col-sm-7">
      <input class="form-control" name="Quantity" value="<%= model.Quantity %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="QC_Status">QC_Status</label>
    <div class="col-sm-7">
      <input class="form-control" name="QC_Status" value="<%= model.QC_Status %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Comments</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" name="Comments" maxlength="32000" rows="7"><%= model.Comments %></textarea>
    </div>
  </div>