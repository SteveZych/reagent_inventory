<!--this file is the /bench route and will display different instrument and link to the instrument endpoint-->
<%- include("_header") -%>
    <% for (const obj of model){ %>
        <% for (const instrument in obj){  %>
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <a href="/instrument/<%= obj[instrument]%>" class=""><h5 class="card-title"><%= obj[instrument] %></h5></a>
                    <!--Progress bar-->
                </div>
            </div>
        <% } %>
    <% } %>
<%- include("_footer") -%>